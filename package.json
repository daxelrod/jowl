{
  "name": "jowl",
  "version": "1.0.0",
  "description": "CLI for JSON operations with Lodash",
  "bin": "src/bin/jowl.js",
  "dependencies": {
    "commander": "^2.9.0",
    "console.json": "^0.2.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "cross-spawn": "^5.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "gulp": "^3.9.0",
    "markdownlint-cli": "^0.15.0",
    "mocha": "^6.2.2",
    "sinon": "^2.1.0"
  },
  "engines": {
    "node": "^6.0.0 || ^8.0.0 || ^10.0.0"
  },
  "scripts": {
    "test": "yarn run test:unit && yarn run test:integration",
    "test:unit": "mocha 'test/unit/**/*.js'",
    "test:integration": "mocha 'test/integration/**/*.js'",
    "lint": "eslint '*.js' '*src/**/*.js', 'test/**/*.js'",
    "docs": "markdownlint --config markdownlint.json '*.md' 'docs/**/*.md'",
    "build": "gulp && yarn run lint && yarn run docs && yarn run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daxelrod/jowl.git"
  },
  "keywords": [
    "cli",
    "json",
    "lodash"
  ],
  "author": "Daniel Axelrod <git@danonline.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daxelrod/jowl/issues"
  },
  "homepage": "https://github.com/daxelrod/jowl#readme",
  "files": [
    "README.md",
    "src"
  ]
}
