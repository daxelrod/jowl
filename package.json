{
  "name": "jowl",
  "version": "2.2.0",
  "description": "CLI for JSON operations with Lodash",
  "bin": "src/bin/jowl.js",
  "dependencies": {
    "commander": "^6.2.1",
    "console.json": "^0.2.1",
    "json-colorizer": "^2.2.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chalk": "^2.4.1",
    "cross-spawn": "^6.0.5",
    "eslint": "^5.0.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.2.0",
    "markdownlint-cli": "^0.15.0",
    "mocha": "^6.2.2",
    "sinon": "^7.5.0"
  },
  "engines": {
    "node": "^6.0.0 || ^8.0.0 || ^10.0.0 || ^12.0.0 || ^14.0.0 || ^16.0.0 || ^18.0.0",
    "yarn": "^1.0.0"
  },
  "scripts": {
    "test": "yarn run test:unit && yarn run test:integration",
    "test:unit": "mocha \"test/unit/**/*.js\"",
    "test:integration": "mocha \"test/integration/**/*.js\"",
    "lint": "eslint \"src/**/*.js\" \"test/**/*.js\"",
    "docs": "markdownlint \"*.md\" \"docs/**/*.md\"",
    "build": "yarn run lint && yarn run docs && yarn run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daxelrod/jowl.git"
  },
  "keywords": [
    "cli",
    "json",
    "lodash"
  ],
  "author": "Daniel Axelrod <git@danonline.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daxelrod/jowl/issues"
  },
  "homepage": "https://github.com/daxelrod/jowl#readme",
  "files": [
    "README.md",
    "src",
    "!**/.eslintrc.js"
  ]
}
